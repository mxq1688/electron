{
  "name": "electron_app",
  "productName": "electron_app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "package-win32": "electron-forge package --arch x64 --platform win32",
    "make": "electron-forge make",
    "make-win32": "electron-forge make --arch x64 --platform win32",
    "publish": "electron-forge publish",
    "start2": "electron .",
    "package-win": "electron-packager . aiask --platform=win32 --arch=x64  --asar  --out=build --overwrite --no-package-manager --ignore='(.git)' --icon=assets/icons/laihua/logo --version-string.ProductName='奇妙问'",
    "package-linux": "electron-packager . aiask --platform=linux --arch=x64  --asar  --out=build --overwrite --no-package-manager --ignore='(.git)' --icon=assets/icons/laihua/logo -no-sandbox",
    "package-mac": "electron-packager . aiask --platform=darwin --arch=x64  --asar  --out=build --overwrite --no-package-manager --ignore='(.git)' --icon=assets/icons/laihua/logo",
    "create-installer-mac": "electron-installer-dmg ./build/aiask-darwin-x64/aiask.app aiask --out=./build/dmg",
    "package-appdmg": "appdmg appdmg.json build/aiask.dmg",
    "builder-win": "electron-builder --win --x64"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "electron": "32.0.1",
    "electron-builder": "^24.13.3",
    "electron-installer-dmg": "^4.0.0",
    "electron-packager": "^17.1.2"
  },
  "keywords": [],
  "author": {
    "name": "xianqiang.meng",
    "email": "xianqiang.meng@mobvoi.com"
  },
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
